                    }
                });
            }
        }

        function consentApproved(e) {
            const directive = e.detail.find((dir) => dir.id === '50030');
            return directive ? directive.allow : true;
        }

        function disableTracking() {
            if (window && window.branch) {
                window.branch.disableTracking(true);
            }
        }

        document.addEventListener('consent-event', function(e) {
            initializeBranch(!consentApproved(e));
            if (!consentApproved(e)) { disableTracking(); }
        });
    </script>
    <script>
      if ('csm' in window) {
        csm.measure('csm_RenderBranchSDK_finished');
      }
    </script>
<script>
    if (typeof uet == 'function') {
      uet("be", "RenderBranchSDK", {wb: 1});
    }
</script>
<script>
    if (typeof uex == 'function') {
      uex("ld", "RenderBranchSDK", {wb: 1});
    }
</script>

<script>
    if (typeof uet == 'function') {
      uet("be", "LoadFooterJS", {wb: 1});
    }
</script>
<script>
    if (typeof uex == 'function') {
      uex("ld", "LoadFooterJS", {wb: 1});
    }
</script>
        
        <div id="servertime" time="107"/>



<script>
    if (typeof uet == 'function') {
      uet("be");
    }
</script>
        
    </body>
